{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://groundmounts.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://groundmounts.com http://localhost:* http://192.168.1.50:3000"
        }
      ]
    }
  ]
}
